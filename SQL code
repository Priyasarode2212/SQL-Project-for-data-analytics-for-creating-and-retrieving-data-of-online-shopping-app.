  select * from Meesho;
  
#Minimum Age
select min(Age)
from Meesho;

select avg(rating)
from Meesho;

select sum(Age)
from Meesho;

select CustID, count(*) as total_customer
from Meesho
group by CustID;

select Age, sum(rating) as total_rating
from Meesho
group by Age
order by total_rating desc 
limit 4 ;

select ProductName, sum(rating) as total_rating
from Meesho
group by ProductName
having total_rating > 8;

select city, sum(rating) as total_loss
from Meesho
group by city
order by total_loss 
limit 5 ;

select *
from Meesho
where Age > 20 ;

create table customer_info (CustID int, Salary numeric, Address varchar(50), Mobile int);

 
select *
from customer_info;

insert into customer_info (CustID, Salary, Address, Mobile)
values (1,'100000','abc',545),
	   (2,'50000','xyz',407),
       (3,'45000','xyz',980),
       (4,'20000','xyz',867),
       (5,'25000','xyz',956),
       (6,'40000','cde',734);
       
select *
from customer_info;

select m.*, ci.Mobile
from Meesho m
inner join customer_info ci
on m.CustID = ci.CustID;
    
    
select m.*,ci.Mobile
from Meesho m
left join customer_info ci
on m.CustID = ci.CustID;

select m.*,ci.Mobile
from Meesho m
right join customer_info ci
on m.CustID = ci.CustID;

select m.*, ci.*
from Meesho m 
left join  customer_info ci
on m.CustID = ci.CustID
union
select m.*,ci.*
from  Meesho m 
right join  customer_info ci
on m.CustID = ci.CustID;






