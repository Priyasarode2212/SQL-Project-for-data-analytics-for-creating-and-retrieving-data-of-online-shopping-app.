CREATE TABLE Meesho
(CustID int PRIMARY KEY,CustName varchar(90) NOT NULL, Age int NOT NULL, City char(90), Country char(90), ProductName varchar(90), ProductID int, Pincode int, rating int );

select * from Meesho;

